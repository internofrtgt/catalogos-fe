{
  "version": 2,
  "builds": [
    {
      "src": "api.ts",
      "use": "@vercel/node"
    },
    {
      "src": "backoffice-admin/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/backoffice-admin/$1"
    }
  ]
}